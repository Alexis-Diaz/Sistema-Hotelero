@model SysHotel.EL.RolUsuario

@{
    ViewBag.Title = "Detalles del usuario";
}
<div class="container">
    <div class="row">
        <div class="col s6">
            <h4>Detalles</h4>
            <hr />
            <div class="card blue lighten-5">
                <div class="card-content grey-text text-darken-4">
                    <span class="card-title">Descripción del rol seleccionado</span>
                    <p>
                        <strong>@Html.DisplayNameFor(model => model.Rol):</strong>
                        @Html.DisplayFor(model => model.Rol)
                    </p>
                    <br />
                </div>
                <div class="card-action">
                    <a class="btn waves-effect waves-light light-blue lighten- white-text" href="~/RolUsuario/Edit/@Model.IdRolUsuario">Editar rol usuario</a>
                </div>
            </div>

        </div>

        <div class="col s6">

        </div>
    </div>
    <a href="~/RolUsuario"><i class="material-icons">undo</i> Regresar a la lista</a>
</div>
