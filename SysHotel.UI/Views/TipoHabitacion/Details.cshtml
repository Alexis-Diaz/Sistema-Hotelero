@model SysHotel.EL.TipoHabitacion

@{
    ViewBag.Title = "Detalles del tipo de habitación";
}

<div class="container">
    <div class="row">
        <div class="col s6">
            <h4>Detalles</h4>
            <hr />
            <div class="card blue lighten-5">
                <div class="card-content grey-text text-darken-4">
                    <span class="card-title">Descripción del tipo de habitación seleccionado</span>
                    <p>
                        <strong>@Html.DisplayNameFor(model => model.TipoDeHabitacion):</strong>
                        @Html.DisplayFor(model => model.TipoDeHabitacion)
                    </p>
                    <br />
                    <p>
                        <strong>@Html.DisplayNameFor(model => model.Descripcion):</strong>
                        @Html.DisplayFor(model => model.Descripcion)
                    </p>
                </div>
                <div class="card-action">
                    <a class="btn waves-effect waves-light light-blue lighten- white-text" href="~/TipoHabitacion/Edit/@Model.IdTipoDeHabitacion">Editar tipo de Habitación</a>
                </div>
            </div>

        </div>

        <div class="col s6">

        </div>
    </div>
    <a href="~/TipoHabitacion"><i class="material-icons">undo</i> Regresar a la lista</a>
</div>
